{
  "name": "@henskelis/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "check:types": "tsc --emitDeclarationOnly false --noEmit",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist",
    "build:source": "./script/build.ts",
    "build:types": "tsc -p tsconfig.build.json",
    "build": "pnpm clean && pnpm build:source && pnpm build:types"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "default": "./dist/*/index.js"
    }
  },
  "dependencies": {
    "@henskelis/lib": "workspace:",
    "lit": "catalog:"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "catalog:",
    "@open-wc/testing": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "commander": "catalog:",
    "esbuild": "catalog:",
    "jsdom": "catalog:",
    "tslib": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "author": "Henskelis",
  "license": "MIT",
  "packageManager": "pnpm@9.12.1",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.5.0"
  }
}
